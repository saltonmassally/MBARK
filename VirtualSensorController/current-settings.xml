  <SensorTimeoutsAreAlwaysFailures>true</SensorTimeoutsAreAlwaysFailures>
  <SensorCancellationAreAlwaysFailures>false</SensorCancellationAreAlwaysFailures>
  <UninitializeSensorsOfCorruptImages>false</UninitializeSensorsOfCorruptImages>
  <WindowLocation>
    <X>10</X>
    <Y>10</Y>
  </WindowLocation>
  <WindowSize>
    <Width>800</Width>
    <Height>600</Height>
  </WindowSize>
